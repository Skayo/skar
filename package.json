{
  "name": "skar",
  "description": "A awful tool for collecting many files into one archive file",
  "version": "1.0.0",
  "author": "Skayo",
  "bin": {
    "skar": "./bin/run"
  },
  "bugs": "https://github.com/Skayo/skar/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "binary-parser-encoder": "^1.5.3",
    "cli-ux": "^5.5.1",
    "fast-glob": "^3.2.5",
    "fs-extra": "^9.1.0",
    "micromatch": "^4.0.2",
    "stat-mode": "^1.0.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/fs-extra": "^9.0.7",
    "@types/micromatch": "^4.0.1",
    "@types/node": "^10.17.54",
    "globby": "^10.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Skayo/skar",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "skar",
    "plugins": [
